  "productName": "Electron LegalLinQ prototype app",
  "version": "0.0.4",
  "description": "Angular PWA Starter met Webpack en VScode 2017 (1.13)",
  "main": "electron-main.js",
  "private": true,
  "scripts": {
    "electron-packager-dev": "tsc electron-main.ts --outDir . && cross-env NODE_ENV=dev webpack-dev-server --config webpack.electron.js --progress --port=4200",
    "electron-packager-prod": "rimraf dist && webpack --config webpack.electron.js --progress && electron dist/electron.js",
    "electron-packager-run-dist": "electron dist/electron.js",
    "electron-packager-packager:win": "cross-env NODE_OS=win node electron-packager-builder/packager.js",
    "electron-packager-packager:mac": "cross-env NODE_OS=mac node electron-packager-builder/packager.js",
    "electron-packager-packager:linux": "cross-env NODE_OS=linux node electron-packager-builder/packager.js",
    "electron-packager-installer:win": "npm run electron-packager-packager:win && cross-env NODE_OS=win node electron-packager-builder/installer.js",
    "electron-packager-installer:mac": "npm run electron-packager-packager:mac && cross-env NODE_OS=mac node electron-packager-builder/installer.js",
    "electron-packager-installer:linux": "npm run electron-packager-packager:linux && cross-env NODE_OS=linux node electron-packager-builder/installer.js",
    "start": "rimraf dist && webpack-dev-server --config webpack.dev.js --hot --inline --progress --port 8080",
    "build": "rimraf dist && webpack --config webpack.prod.js --bail --colors --display-error-details --progress --profile",
    "build:jsonlog": "rimraf dist && webpack --config webpack.prod.js --bail --colors --display-error-details --progress --profile --json > webpack-build-log.json",
    "build:extra-memory": "rimraf dist && node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack.js --config webpack.prod.js --colors --progress --profile",
    "build:electron": "rimraf dist && webpack --config webpack.electron.js --progress",
    "tsc:w": "tsc -w",
    "compodoc": "compodoc -p tsconfig.json"
  },
